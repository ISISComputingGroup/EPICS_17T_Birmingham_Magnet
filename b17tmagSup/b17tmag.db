record(bo, "$(P)DISABLE"){
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "$(DISABLE=0)")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}


record(ai, "$(P)FIELD"){
    field(DESC, "The field of the magnet at present")
    field(DTYP, "stream")
    field(PREC, "2")
    field(INP, "$(P)SEND_FIELD_CURR_VALS") # TEST
    # field(INP, "@b17tmag.proto getField($(P)) $(PORT)") # OLD
    field(SCAN, "1 second")
    field(EGU, "T")
}

record(ai, "$(P)CURR"){
    field(DESC, "The current of the magnet at present")
    field(DTYP, "stream")
    field(PREC, "2")
    field(INP, "$(P)SEND_FIELD_CURR_VALS") # TEST
    # field(INP, "@b17tmag.proto getField($(P)) $(PORT)") # OLD
    field(EGU, "A")
}


# Waveform to read in FIELD and CURR PV's in and fanout record
record(waveform, "$(P)FIELD_CURR_ARRAY"){
    field(DESC, "Array ot store FELD and CURR VALS")
    field(DTYP, "stream")
    field(SCAN, "1 second")
    field(INP,  "@b17tmag.proto getField($(P)) $(PORT)")
    field(NELM, "2")
    field(FTVL, "DOUBLE")
    field(FLNK, "$(P)SEND_FIELD_CURR_VALS")
}

# Fanout record to pass FIELD and CURR vals over to correct PV's
record(fanout, "$(P)SEND_FIELD_CURR_VALS"){
    field(DESC, "Send to respective PVs")
    field(SCAN, "Passive")
    field(LNK1, "$(P)FIELD")
    field(LNK2, "$(P)CURR")
}


record(mbbi, "$(P)HEATER"){
    field(DESC, "The status of the heater")
