# Needs to be converted to a percentge of 243 
# (divide the returned value by 243 then multiply by 100) 
# - maybe possible at the proto file level
# HE LEVEL Raw Input Value
record(ai, "$(P)HE:LEVEL_RAW"){
    field(DESC, "The current level of helium")
    field(DTYP, "stream")
    field(SCAN, "1 second")
    field(PREC, "2")
    field(EGU, "%")
    field(INP, "@b17tmag.proto getHeLevel $(PORT)")
}

# HE LEVEL Calc Value as Pecentage of 243
record(calcout, "$(P)HE:LEVEL:CALC"){
    field(DESC, "Percentage value of HE:LEVEL")
    field(INPA, "$(P)HE:LEVEL_RAW CP MSS")
    field(CALC, "(A / 243) * 100")
}

# HE LEVEL as percetnage of 243
record(ai, "$(P)HE:LEVEL"){
    field(DESC, "The current level of helium")
    field(INP, "$(P)HE:LEVEL:CALC CP MSS")
    field(PREC, "2")
    field(EGU, "%")
    field(VAL, "0") # default value so pv does not display as disabled in OPI
    info(archive, "VAL")
    info(interest, "MEDIUM")
}

